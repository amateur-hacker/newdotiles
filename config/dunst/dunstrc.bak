[global]
monitor = 0
follow = keyboard
origin = top-right
offset = 10x5
indicate_hidden = yes
shrink = no
separator_height = 4
separator_color= frame
separator_color = auto
padding = 15
horizontal_padding = 15
frame_width = 3
frame_color = "#89b4fa"
sort = no
idle_threshold = 120
font = JetBrains Mono 10
line_height = 4
markup = full
format = %s\n%b
alignment = left
show_age_threshold = 60
word_wrap = yes
ignore_newline = no
stack_duplicates = false
hide_duplicate_count = yes
show_indicators = no
icon_position = left
icon_path = /home/amateur_hacker/.local/share/icons/notification-icons
max_icon_size = 48
sticky_history = yes
history_length = 20
browser = zen-browser --new-tab
always_run_script = true
title = Dunst
class = Dunst
corner_radius = 8
highlight = "#7f7fff"
# close = ctrl+space
# close_all = ctrl+shift+space
# history = ctrl+grave
# context = ctrl+shift+period

